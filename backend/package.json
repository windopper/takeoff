{
	"name": "takeoff-backend",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"cf-typegen": "wrangler types",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"db:init": "wrangler d1 execute takeoff --remote --file=./schema.sql",
		"db:local-init": "wrangler d1 execute takeoff --local --file=./schema.sql",
		"db:migrate": "wrangler d1 execute takeoff --remote --file=./schema.sql",
		"db:local-migrate": "wrangler d1 execute takeoff --local --file=./schema.sql",
		"db:shell": "wrangler d1 execute takeoff --remote --command=\"SELECT name FROM sqlite_master WHERE type='table';\"",
		"db:local-shell": "wrangler d1 execute takeoff --local --command=\"SELECT name FROM sqlite_master WHERE type='table';\""
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/node": "^24.0.1",
		"jest": "^30.0.0",
		"ts-jest": "^29.4.0",
		"typescript": "^5.5.2",
		"wrangler": "^4.20.0"
	},
	"dependencies": {
		"@langchain/core": "^0.3.0",
		"@langchain/google-genai": "^0.2.0",
		"@langchain/openai": "^0.3.0",
		"langchain": "^0.3.0"
	}
}
